# Git
alias gpp="git pull --prune"
alias gl="git log --oneline -n"
alias gcm='git checkout $(git rev-parse --verify main &>/dev/null && echo "main" || echo "master")'
alias gcam='git commit -am "$(date +"%Y-%m-%d %H:%M:%S")"'
alias gmm='git merge $(git rev-parse --verify main &>/dev/null && echo "main" || echo "master")'
alias gc-='git checkout -'
alias gsp="git stash pop"
alias gss="git stash save -u "

# Docker
alias dsss="docker-sync-stack start"

# Misc.
alias yt-audio="yt-dlp -x --audio-format m4a --write-thumbnail --embed-thumbnail "
alias ls='ls --color=auto'
